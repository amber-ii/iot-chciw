<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{title}}</title>
  <link rel="stylesheet" type="text/css" href="./css/animate.min.css">
  <link rel="stylesheet" type="text/css" href="./css/pattern.css">
  <link rel="stylesheet" href="./css/style.css">
  <link rel="stylesheet" type="text/css" href="./css/sacid.css">
  <link rel="stylesheet" type="text/css" href="./css/progress.css">

  <link
    href="https://fonts.googleapis.com/css?family=Material+Icons|Material+Icons+Outlined|Material+Icons+Two+Tone|Material+Icons+Round|Material+Icons+Sharp"
    rel="stylesheet">


</head>
<!-- CH1ACI -->

<body onload="subTopic='CH1ACI';MQTTConnect();">
  <a href="/logout" class="flex justify-end mt-3 mr-5">
    <button
      class="text-lg font-light outline-none focus:outline-none transform transition-all hover:scale-110 text-yellow-500 hover:text-yellow-600 ">登出
      <i class="mdi mdi-logout -m-1"></i>
    </button>
  </a>

  <div class="demo">
    <div class="demo__colored-blocks">
      <div class="demo__colored-blocks-rotater">
        <div class="demo__colored-block"></div>
        <div class="demo__colored-block"></div>
        <div class="demo__colored-block"></div>
      </div>
      <div class="demo__colored-blocks-inner"></div>
      <div class="demo__text">載入中</div>
    </div>
    <div class="demo__inner">
      <svg class="demo__numbers" viewBox="0 0 100 100">
        <defs>
          <path class="demo__num-path-1" d="M40,28 55,22 55,78" />
          <path class="demo__num-join-1-2" d="M55,78 55,83 a17,17 0 1,0 34,0 a20,10 0 0,0 -20,-10" />
          <path class="demo__num-path-2" d="M69,73 l-35,0 l30,-30 a16,16 0 0,0 -22.6,-22.6 l-7,7" />
          <path class="demo__num-join-2-3" d="M28,69 Q25,44 34.4,27.4" />
          <path class="demo__num-path-3" d="M30,20 60,20 40,50 a18,15 0 1,1 -12,19" />
        </defs>
        <path class="demo__numbers-path" d="M-10,20 60,20 40,50 a18,15 0 1,1 -12,19 
               Q25,44 34.4,27.4
               l7,-7 a16,16 0 0,1 22.6,22.6 l-30,30 l35,0 L69,73 
               a20,10 0 0,1 20,10 a17,17 0 0,1 -34,0 L55,83 
               l0,-61 L40,28" />
      </svg>
    </div>
  </div>



  <section class="text-gray-100">
    <div class="max-w-full mx-auto pb-10 px-8">
      <div class="include" file="menu.html"></div>
      <div class="pageBar1  sticky top-1 -m-8 ml-14">A25</div>
  
   

      <!-- table -->
      <div class="bg-gray-900 flex items-center justify-center mt-10 mb-6">
        <div class="flex items-center w-full lg:w-11/12">
          <div class="col-span-12">
            <div class="overflow-auto lg:overflow-visible ">

              {{!-- download button --}}
              <div class="flex items-center">
                <span class="inline-block  border-2 rounded-md border-gray-600 px-3 pb-2 -mb-1">
                  {{!-- <form id="Form1" action="/sacid" method="post"> --}}
                    <div class="flex inline-block">
                      <form id="form1" name="dateSearch">
                        <div class="flex flex-nowrap">
                          <span class="space-x-1 mr-6 pl-2 inline-block pt-3 ">
                            <label for="startDate">起始日</label>
                            <input type="date" name="startDate" id="startDate" class="text-gray-700 text-center"
                              min="2020-07-01" required>
                          </span>
                          <span class="space-x-1 mr-6 pt-3 ">
                            <label for="startDate">結束日</label>
                            <input type="date" name="endDate" id="endDate" class="text-gray-700 text-center" required>
                          </span>
                          <div title="查詢">
                            <type class="pt-3 hover:text-gray-100 search" id="search" type="button">
                              <svg xmlns="http://www.w3.org/2000/svg" width="26" height="26" fill="currentColor"
                                class="bi bi-search" viewBox="0 0 16 16">
                                <path
                                  d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z" />
                              </svg>
                            </type>
                          </div>
                        </div>
                      </form>


                      {{!-- Download Report --}}
                      <span title="導出報表">
                        <button id="btnExport" class="download pt-3 pr-2 ml-6 hover:text-gray-100"
                          onclick="exportTable()">
                          <svg xmlns="http://www.w3.org/2000/svg" width="27" height="27" fill="currentColor"
                            class="bi bi-download" viewBox="0 0 16 16">
                            <path
                              d="M.5 9.9a.5.5 0 0 1 .5.5v2.5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2.5a.5.5 0 0 1 1 0v2.5a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2v-2.5a.5.5 0 0 1 .5-.5z" />
                            <path
                              d="M7.646 11.854a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V1.5a.5.5 0 0 0-1 0v8.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3z" />
                          </svg>
                        </button>
                      </span>
                    </div>



              </div>

              {{!-- download button --}}

              <table class="table text-gray-200 border-separate space-y-5" id="headerTable">
                <thead class="text-gray-100 text-sm sticky top-0" style="background-color:Mediumblue">
                  <tr>
                    <th class=" text-center py-4">日期</th>
                    <th class="text-center py-4">item</th>
                    <th class=" text-center">item</th>
                    <th class=" text-center">item</th>
                    <th class=" text-center">item</th>
                    <th class=" text-center">item</th>
                    <th class=" text-center">item</th>
                    <th class="text-center pr-3">item</th>
                  </tr>
                </thead>
                {{#each rows}}
                <tbody id="table2">
                </tbody>
                <tbody id="table1">
                  <tr class="bg-gray-800 text-md 2xl:text-xl">
                    <th class="p-3 text-center dateTh">{{this.preDate}}-{{this.Date}}</th>
                    <th class="p-3 text-center">{{this.A9Diff}}</th>
                    <th class="p-3 text-center">{{this.A10Diff1}}</th>
                    <th class="p-3 text-center">{{this.A10Diff4}}</th>
                    <th class="p-3 text-center">{{this.total}}</th>
                    <th class="p-3 text-center">{{this.A10Diff2}}</th>
                    <th class="p-3 text-center">{{this.A10Diff3}}</th>
                    <th class="p-3 text-center">{{this.diff32}}</th>
                  </tr>
                </tbody>



                {{/each}}


              </table>

            </div>
          </div>
        </div>
      </div>
    </div>


    {{!-- <div class="mb-5 mr-5 flex justify-end">
      <a href="/logout"><button
          class="text-lg font-light outline-none focus:outline-none transform transition-all hover:scale-110 text-yellow-500 hover:text-yellow-600 ml-10">登出<i
            class="mdi mdi-arrow-right "></i></button></a>
    </div> --}}
  </section>


</body>

